    "tsx": "^3.12.8",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vue-tsc": "^1.8.10"
  },
  "dependencies": {
    "@types/node": "^20.5.9",
    "vue": "^3.3.4"
  }
}
